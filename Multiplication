  <center> <h2> <p> <B>CALCULATE MULTIPLICATION </P> </center></h2>

 <center>
    
<p>
    
    Enter a number to find the multiplication: <input type="number" id="numValue" maxlength ="64"/>
</p>
<p>
    Enter a second number to multiply: <input type="number" id="numValue2" maxlength="64"/>
</p>
<p>
    <button type="button" onclick="fvMultiply()">Go</button>
</p>
<p id="txtOutput">Click Go</p>
<script>
    function fvMultiply()
    {
        var dInput1;
        var dInput2;    // Variable to store the value typed into the textbox multiplication
        var dOut;       // Double variable to store output to the web page
        
        dInput1 = numValue.value;
        dInput2 = numValue2.value;

        dOut = dInput1 * dInput2;
        
        // Output the final string to the web page:
        document.getElementById("txtOutput").innerHTML = dOut;

       return;
    }

 </script>
